<% if notice %>
  <p id="notice"><%= notice %></p> 
<% end %>

<h1>Listing Users</h1>

<% @users.each do |user| %>

  <p><%= user.name %>
  <%= link_to 'Show', user %> | 
  <%= link_to ' Edit', edit_user_path(user) %> | 
  <%= link_to ' Destroy', user, method: :delete, data: { confirm: 'Are you sure?' } %></p>

<% end %>
 

<br>

<%= link_to 'New User', new_user_path %>
